{{js "js/index.js"}}
    <div class="row">
            <div id="endpoint-listing-welcome-message">
                <div class="message message-success">
                    <h4><i class="icon fw fw-success"></i>No Endpoints to list</h4>
                    <p>{{i18n "You.don't.have.any.Endpoints.to.list.here.Click.on.the"}}<b>{{i18n "Add"}}
                    </b>{{i18n " button.above.to.add.new.Endpoint."}}</p>
                </div>
            </div>
        <div class="hidden" id="endpoint-listing-container">
            <div class="content-section shadow-up">
                <div class="content-data">
                    <div class="page-content">
                        <table id="endpoint-listing" class="table table-striped table-bordered"
                               cellspacing="0"
                               width="100%">
                            <thead>
                            <tr>
                                <th>{{i18n "Endpoint.Name"}}</th>
                                <th>{{i18n "Endpoint.Type"}}</th>
                                <th class="endpoint-service-url">{{i18n "Endpoint.ServiceUri"}}</th>
                                <th class="endpoint-security">{{i18n "Endpoint.Security"}}</th>
                                <th class="endpoint-action">{{i18n "Actions"}}</th>
                            </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>